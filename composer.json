{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "5.3.*",
        "barryvdh/laravel-debugbar": "^2.2",
        "intervention/image": "^2.3",
        "devfactory/minify": "1.0.*",
        "cviebrock/eloquent-sluggable": "^4.0",
        "bkwld/croppa": "^4.4",
        "spatie/eloquent-sortable": "^2.1",
        "jenssegers/date": "^3.2",
        "zizaco/entrust": "5.2.x-dev",
        "laravelcollective/html": "^5.3",
        "yajra/laravel-datatables-oracle": "~6.0",
        "barryvdh/laravel-translation-manager": "0.2.x",
        "dimsav/laravel-translatable": "^6.0",
        "spatie/laravel-analytics": "^1.4",
        "mcamara/laravel-localization": "1.1.*",
        "anlutro/l4-settings": "^0.4.9",
        "watson/rememberable": "^2.0",
        "predis/predis": "^1.1",
        "unikent/tfilecache": "^2.0",
        "mpdf/mpdf": "^7.0",
        "maatwebsite/excel": "~2.1.0",
        "lynx39/lara-pdf-merger": "1.0.0",
        "econea/nusoap": "v0.9.5.3",
        "webit/gls-tracking": "^1.0",
        "laravel/passport": "~1.0.18",
        "tplaner/when": "^2.1",
        "pusher/pusher-php-server": "^3.0",
        "webklex/laravel-imap": "^2.4",
        "jeremy-dunn/php-fedex-api-wrapper": "^2.6.2",
        "200mph/tnt-express-connect": "^2.0",
        "brotzka/laravel-dotenv-editor": "^2.1",
        "mpdf/qrcode": "^1.0",
        "doctrine/dbal": "^2.9",
        "phpseclib/phpseclib": "^2.0",
        "lcobucci/jwt": "3.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
